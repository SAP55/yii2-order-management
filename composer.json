{
    "name": "sap55/order",
    "description": "Orders Menagment System",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "SAP55",
            "email": "serega.pisarev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.*",
        "yiisoft/yii2-bootstrap": "2.*",
        "yiisoft/yii2-composer": "*",
        "yiisoft/yii2-swiftmailer": "2.*",
        "voskobovich/yii2-many-many-behavior": "*",
        "nemmo/yii2-attachments": "dev-master",
        "kartik-v/yii2-dynagrid": "*",
        "kartik-v/yii2-date-range": "*",
        "kartik-v/yii2-krajee-base": "dev-master",
        "2amigos/yii2-gallery-widget": "*",
        "kartik-v/yii2-grid": "dev-master",
        "kartik-v/yii2-mpdf": "dev-master",
        "kartik-v/yii2-export": "*",
        "kartik-v/yii2-editable": "*",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-helpers": "dev-master",
        "pheme/yii2-settings": "*",
        "rezzza/formulate": "~1.0",
        "kartik-v/yii2-builder": "dev-master",
        "insolita/yii2-adminlte-widgets": "dev-master",
        "ruskid/yii2-ip-behavior": "dev-master",
        "devgroup/yii2-tag-dependency-helper": "dev-master"
        "jlorente/yii2-command-handler": "*",
    },
    "autoload": {
        "psr-4": {
            "sap55\\order\\": ""
        }
    },
    "extra": {
        "bootstrap": "sap55\\order\\Bootstrap",
        "branch-alias": {
            "dev-master": "1.0.0-dev",
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
